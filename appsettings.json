{
  "Kafka": {
    "BootstrapServers": "<CONFLUENT_CLOUD_BOOTSTRAP_SERVER>",
    "SecurityProtocol": "SaslSsl",
    "SaslMechanism": "Plain",
    "SaslUsername": "<API_KEY>",
    "SaslPassword": "<API_SECRET>",
    "Acks": "Leader",
    "LingerMs": 100,
    "BatchSize": 1000000,
    "CompressionType": "Lz4"
  },
  "SchemaRegistry": {
    "Url": "<SCHEMA_REGISTRY_URL>",
    "BasicAuthUserInfo": "<SR_API_KEY>:<SR_API_SECRET>"
  },
  "Test": {
    "MessageCount": 100000,
    "DurationMinutes": 1,
    "ProducerRuns": 3,
    "ConsumerRuns": 3,
    "BatchTimeoutMs": 500,
    "CommitBatchSize": 100,
    "CommitIntervalMs": 500,
    "InterMessageDelayMs": 100,
    "TopicPrefix": "test-",
    "AvroSmallSpecificTopic": "test-avro-small-specificrecord",
    "AvroLargeSpecificTopic": "test-avro-large-specificrecord",
    "JsonSmallTopic": "test-json-small",
    "JsonLargeTopic": "test-json-large"
  }
}
