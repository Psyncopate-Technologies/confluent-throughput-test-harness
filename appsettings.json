    "ConsumerRuns": 5,
    "TopicPrefix": "throughput-test",
    "AvroSmallTopic": "throughput-test-avro-small",
    "AvroLargeTopic": "throughput-test-avro-large",
    "JsonSmallTopic": "throughput-test-json-small",
    "JsonLargeTopic": "throughput-test-json-large"
  }
}
